{
	"version": 2,
	"builds": [
		{ "src": "fiesta-finder/package.json", "use": "@vercel/static-build", "config": { "distDir": "build", "installCommand": "npm ci", "buildCommand": "npm run build" } }
	],
	"routes": [
		{ "handle": "filesystem" },
		{ "src": "/api/(.*)", "dest": "https://REPLACE_WITH_BACKEND_HOST/api/$1" },
		{ "src": "/(.*)", "dest": "fiesta-finder/build/$1" },
		{ "src": "/.*", "dest": "fiesta-finder/build/index.html" }
	]
}


